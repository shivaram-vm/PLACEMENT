
#include <stdio.h>

int main()
{
int num,i,isprime=1;
printf("Enter an integer:");
scanf("%d",&num);
if (num<=1){
    isprime=0;
}
else{
    for (i=2;i*i<=num;i++){
        if (num%i==0)
        {
            isprime=0;
            break;
        }
    }
}
if(isprime)
printf("%d is a primr number",num);
else
printf("%d is not a prime number",num);
    return 0;
}
