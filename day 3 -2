int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int a;
    scanf("%d",&a);
    if(a%3==0&&a%5==0)
        printf("HiHello");
    else if(a%3==0)
        printf("Hi");
    else if(a%5==0)
        printf("Hello");
    else
        printf("");
    return 0;
}
