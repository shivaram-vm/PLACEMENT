#include <stdio.h>

int main()
{
    int a, b;
    printf("Enter a number: ");
    scanf("%d", &a);
    
    printf("Enter another number: ");
    scanf("%d", &b);
    
    if (a > b)
        printf("%d is greater than %d\n", a, b);
    else if (a < b)
        printf("%d is greater than %d\n", b, a);
    else
        printf("Both numbers are equal.\n");

    return 0;
}
